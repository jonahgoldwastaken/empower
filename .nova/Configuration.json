{
  "editor.default_syntax" : "svelte",
  "index.ignored_file_patterns" : [
    "node_modules",
    ".netlify",
    ".routify",
    ".nova"
  ],
  "index.use_scm_ignored_files" : true,
  "prettier.default-config.arrowParens" : "avoid",
  "prettier.default-config.bracketSpacing" : "Enable",
  "prettier.default-config.endOfLine" : "lf",
  "prettier.default-config.htmlWhitespaceSensitivity" : "css",
  "prettier.default-config.jsxBracketSameLine" : "Disable",
  "prettier.default-config.printWidth" : 80,
  "prettier.default-config.quoteProps" : "as-needed",
  "prettier.default-config.semi" : "Disable",
  "prettier.default-config.singleQuote" : "Enable",
  "prettier.default-config.tabWidth" : 2,
  "prettier.default-config.trailingComma" : "es5",
  "prettier.default-config.useTabs" : "Disable",
  "prettier.format-on-save" : "Enable",
  "workspace.color" : 3,
  "workspace.name" : "Empower",
  "workspace.preview_append_paths" : false,
  "workspace.preview_type" : "custom",
  "workspace.preview_url" : "http:\/\/localhost:5000"
}
